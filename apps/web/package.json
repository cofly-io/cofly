{
  "name": "web",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack --port 3000",
    "build": "next build && pnpm run copy-node-assets",
    "copy-node-assets": "copyfiles -u 6 ../../packages/node-set/dist/nodes/**/*.{json,svg,png} public/nodes && copyfiles -u 6 ../../packages/node-set/dist/connects/**/*.{json,svg,png} public/connects",
    "start": "next start --port 3000",
    "lint": "next lint --max-warnings 0",
    "check-types": "tsc --noEmit",
    "clean": "powershell -Command \"Remove-Item -Path '.next', '.turbo', 'node_modules' -Recurse -Force -ErrorAction SilentlyContinue\"",
    "db:push": "pnpm --filter @repo/database db:push",
    "db:studio": "pnpm --filter @repo/database db:studio",
    "analyze": "ANALYZE=true next build"
  },
  "browser": {
    "node:process": false,
    "node:stream": false,
    "reflect-metadata": false
  },
  "workspaces": [
    "../../packages/*"
  ],
  "dependencies": {
    "@ag-ui/client": "^0.0.35",
    "@astronautlabs/jsonpath": "^1.1.2",
    "@cofly-ai/interfaces": "workspace:*",
    "@copilotkit/react-core": "1.9.3",
    "@copilotkit/react-ui": "1.9.3",
    "@copilotkit/runtime": "1.9.3",
    "@prisma/client": "^6.12.0",
    "@repo/common": "workspace:*",
    "@repo/database": "workspace:*",
    "@repo/services": "workspace:*",
    "@repo/engine": "workspace:*",
    "@repo/node-set": "workspace:*",
    "@repo/mcp-workflow-tools": "workspace:*",
    "@repo/ui": "workspace:*",
    "@types/bcryptjs": "^3.0.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "sharp": "workspace:*",
    "bcryptjs": "^3.0.2",
    "buffer": "^6.0.3",
    "cors": "^2.8.5",
    "crypto-browserify": "^3.12.1",
    "express": "^5.1.0",
    "next": "^15.4.4",
    "next-auth": "^4.24.11",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.1",
    "remark-gfm": "^4.0.1",
    "stream-browserify": "^3.0.0",
    "styled-components": "^6.1.19",
    "util": "^0.12.5"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.4.7",
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@tailwindcss/postcss": "^4",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^22.16.5",
    "@types/react": "^19.1.8",
    "@types/styled-components": "^5.1.34",
    "copyfiles": "^2.4.1",
    "eslint": "^9.31.0",
    "tailwindcss": "^4",
    "typescript": "5.8.3",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
